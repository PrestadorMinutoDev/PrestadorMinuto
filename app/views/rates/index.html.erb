<p id="notice"><%= notice %></p>

<h1>Listing Rates</h1>

<table>
  <thead>
    <tr>
      <th>Punctuality</th>
      <th>Quality</th>
      <th>Presentation</th>
      <th>Comments</th>
      <th>Ad_ID</th>
      <th>User_ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rates.each do |rate| %>
      <tr>
        <td><%= rate.punctuality %></td>
        <td><%= rate.quality %></td>
        <td><%= rate.presentation %></td>
        <td><%= rate.comments %></td>
        <td><%= rate.ad_id %></td>
        <td><%= rate.user_id %></td>
        <td><%= link_to 'Show', rate %></td>
        <td><%= link_to 'Edit', edit_rate_path(rate) %></td>
        <td><%= link_to 'Destroy', rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rate', new_rate_path %>
